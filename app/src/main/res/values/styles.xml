<resources>
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="background">@color/blue_main</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textColorHint">@color/white</item>
    </style>
    <style name="Theme.AppCompat.Light.DarkActionBar" parent="Base.Theme.AppCompat.Light.DarkActionBar" />
    <style name="Hyperlink">
        <item name="android:textColor">#85adad</item>
        <item name="android:background">@drawable/hyperlink_underline</item>
    </style>
    <style name="no_action" parent="Theme.AppCompat.Light.NoActionBar" />

</resources>
